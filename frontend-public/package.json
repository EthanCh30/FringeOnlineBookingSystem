{
  "name": "fringe-online-booking-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "@fawmi/vue-google-maps": "^0.9.79",
    "axios": "^1.6.2",
    "core-js": "^3.8.3",
    "html2canvas": "^1.4.1",
    "moment": "^2.30.1",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "qrcode-vue": "^1.2.0",
    "qrcode.vue": "^3.6.0",
    "vue": "^3.3.0",
    "vue-qrcode": "^2.2.2",
    "vue-router": "^4.2.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/preset-env": "^7.22.20",
    "@testing-library/vue": "^8.0.1",
    "@vue/babel-plugin-jsx": "^1.4.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.0",
    "@vue/test-utils": "^2.4.5",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "vue-eslint-parser": "^10.1.2",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "vue",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/router/index.js",
      "!**/node_modules/**"
    ]
  }
}
